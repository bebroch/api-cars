{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@sparescsv/sparescsv/(.*)": "<rootDir>/../libs/sparescsv/src/$1",
    "@sparescsv/sparescsv": "<rootDir>/../libs/sparescsv/src"
  }
}